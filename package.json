{
  "name": "@speee/iiopt",
  "description": "(iikanji ni)nice image optimizer ",
  "version": "0.1.4",
  "author": "shuhei.morioka",
  "bin": {
    "iiopt": "./bin/cli"
  },
  "bugs": "https://github.com/speee/iiopt/issues",
  "dependencies": {
    "imagemin": "^5.3.1",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-pngquant": "^5.0.1",
    "meow": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^21.1.10",
    "@types/node": "^8.5.2",
    "execa": "^0.8.0",
    "jest": "^22.0.4",
    "ts-jest": "^22.0.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  },
  "files": [
    "build",
    "bin"
  ],
  "homepage": "https://github.com/speee/iiopt",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "modulePathIgnorePatterns": [
      "/build/",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "keywords": [
    "image",
    "jpeg",
    "jpg",
    "mozjpeg",
    "png",
    "pngquant"
  ],
  "license": "MIT",
  "main": "./build/cli.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/speee/iiopt"
  },
  "scripts": {
    "build": "yarn tsc",
    "lint": "tslint 'src/**/*.ts'",
    "test": "yarn build && yarn jest"
  }
}
